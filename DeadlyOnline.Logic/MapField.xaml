<UserControl x:Class="DeadlyOnline.Logic.MapField"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DeadlyOnline.Logic"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Focusable="True">
    <Grid>
        <local:MapViewer x:Name="mapViewer" x:FieldModifier="private"/>

        <Image x:Name="PlayerImage" Stretch="None" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Grid>
</UserControl>
